{
    "manifest_version": 3,
    "name": "To-Do List",
    "version": "1.0",
    "description": "A to-do list with 30-minute nofications designed for the computer science sphere.",
    "permissions": ["storage", "notifications", "alarms"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}